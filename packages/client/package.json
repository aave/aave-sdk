{
  "name": "@aave/client",
  "version": "0.0.0",
  "description": "The official JavaScript client for the Aave API",
  "repository": {
    "directory": "packages/client",
    "type": "git",
    "url": "git://github.com/aave/aave-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./actions": {
      "types": "./dist/actions/index.d.cts",
      "import": "./dist/actions/index.js",
      "require": "./dist/actions/index.cjs"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.cts",
      "import": "./dist/test-utils.js",
      "require": "./dist/test-utils.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "actions": [
        "./dist/actions/index.d.ts"
      ],
      "ethers": [
        "./dist/ethers/index.d.ts"
      ],
      "viem": [
        "./dist/viem/index.d.ts"
      ],
      "test-utils": [
        "./dist/test-utils.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@aave/env": "workspace:*",
    "@aave/graphql": "workspace:*",
    "@aave/types": "workspace:*",
    "@urql/core": "^5.2.0",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "ethers": "^6.14.4",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "viem": "^2.31.6"
  },
  "peerDependencies": {
    "ethers": "^6.14.4",
    "viem": "^2.31.6"
  },
  "peerDependenciesMeta": {
    "ethers": {
      "optional": true
    },
    "viem": {
      "optional": true
    }
  },
  "license": "MIT"
}
