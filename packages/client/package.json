{
  "name": "@aave/client",
  "version": "0.0.0",
  "description": "The official JavaScript client for the Aave API",
  "repository": {
    "directory": "packages/client",
    "type": "git",
    "url": "git://github.com/aave/aave-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "import": [
        "./dist/index.d.ts"
      ],
      "require": [
        "./dist/index.d.cts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@aave/env": "workspace:*",
    "@aave/graphql": "workspace:*",
    "@aave/types": "workspace:*",
    "@urql/core": "^5.2.0",
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "license": "MIT"
}
