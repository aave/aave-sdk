{
  "name": "@aave/types",
  "version": "0.0.0",
  "description": "Core types for the Aave SDK",
  "repository": {
    "directory": "packages/types",
    "type": "git",
    "url": "git://github.com/aave/aave-sdk.git"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./actions": {
      "types": "./dist/actions/index.d.cts",
      "import": "./dist/actions/index.js",
      "require": "./dist/actions/index.cjs"
    },
    "./test-utils": {
      "types": "./dist/test-utils.d.cts",
      "import": "./dist/test-utils.js",
      "require": "./dist/test-utils.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "actions": [
        "./dist/actions/index.d.ts"
      ],
      "ethers": [
        "./dist/ethers/index.d.ts"
      ],
      "viem": [
        "./dist/viem/index.d.ts"
      ],
      "test-utils": [
        "./dist/test-utils.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "license": "MIT"
}
